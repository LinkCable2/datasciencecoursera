#1) Plot the 30-day mortality rates for heart attack##Read the File##Change the mortality rate column to numericoutcome[, 11] <- as.numeric(outcome[, 11])##Plot the histogram of the 30-day mortality rates for heart attackhist(outcome[, 11])##Best.Rbest <- function(st, out) {  ##read outcome data  outcome <- read.csv('Coursera Hospital Data/outcome-of-care-measures.csv', colClasses = "character")  train <- outcome[, c(2, 7, 11, 17, 23)]    ##Check that state and outcome are valid  if (!(st %in% outcome$State)){    stop("invalid state")}  if (!(out %in% c("heart attack", "heart failure", "pneumonia"))){    stop("invalid outcome")}       ##Return hospital name in that state with the lower 30-day death rate  #change column names  colnames(train) <- c("hospital.name", "state", "heart.attack", "heart.failure", "pneumonia")    #subset only useful informaticon on outcomes  x <- subset(train, state == "TX")  #Check what outcome   if (out == "heart attack") {    #Remove NAs and Not Availables    x <- x[!(x$heart.attack == "NA" | x$heart.attack == "Not Available"), ]    #List the hospitals with the lowest heart attack death rate    a <- x$hospital.name[as.numeric(x$heart.attack) == as.numeric(min(x$heart.attack, na.rm = T))]    #Order by name    a <- a[order(a)]}    if (out == "heart failure") {    #Remove NAs and Not Availables    x <- x[!(x$heart.failure == "NA" | x$heart.failure == "Not Available"), ]    #List the hospitals with the lowest heart attack death rate    a <- x$hospital.name[as.numeric(x$heart.failure) == as.numeric(min(x$heart.failure, na.rm = T))]    #Order by name    a <- a[order(a)]}    if (out == "pneumonia"){    #Remove NAs and Not Availables    x <- x[!(x$pneumonia == "NA" | x$pneumonia == "Not Available"), ]    #List the hospitals with the lowest heart attack death rate    a <- x$hospital.name[as.numeric(x$pneumonia) == as.numeric(min(x$pneumonia, na.rm = T))]    #Order by name    a <- a[order(a)]}  print(a[1])                 }    
